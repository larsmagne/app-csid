<?xml version='1.0' encoding='utf-8'?>
<widget id="no.ingebrigtsen.csid.CSID--Concerts" version="0.0.11" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Concerts in Oslo</name>
    <description>
        Concerts in Oslo
    </description>
    <author email="larsi@gnus.org" href="http://lars.ingebrigtsen.no/">
        Lars Magne Ingebrigtsen
    </author>
    <preference name="show-splash-screen-spinner" value="false" />
    <gap:splash src="splash.png" />
    <gap:splash src="splash.png" gap:platform="ios" width="320" height="480" />
    <gap:splash src="splash.png" gap:platform="ios" width="640" height="960" />

    <!-- iPhone 5 / iPod Touch (5th Generation) -->
    <gap:splash src="splash.png" gap:platform="ios" width="640" height="1136" />
    
    <!-- iPad -->
    <gap:splash src="splash.png" gap:platform="ios" width="768" height="1024" />
    <gap:splash src="splash.png" gap:platform="ios" width="1024" height="768" />
    
    <!-- Retina iPad -->
    <gap:splash src="splash.png" gap:platform="ios" width="1536" height="2048" />
    <gap:splash src="splash.png" gap:platform="ios" width="2048" height="1536" />

    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="www/csid-icon.png" />
    <icon src="www/csid-icon.png" platform="android" density="ldpi" />
    <icon src="www/csid-icon.png" platform="android" density="mdpi" />
    <icon src="www/csid-icon.png" platform="android" density="hdpi" />
    <icon src="www/csid-icon.png" platform="android" density="xhdpi" />
    <icon src="www/csid-icon.png" platform="ios" width="512" height="512" density="mdpi" />
</widget>
